            </if>
            <if test="districtId != null">
                district_id = #{districtId, jdbcType=BIGINT},
            </if>
            <if test="medicalInstitution != null and medicalInstitution != ''">
                medical_institution = #{medicalInstitution, jdbcType=VARCHAR},
