{
    "name": "recruitment-view",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve --mode dev",
        "build-dev": "vue-cli-service build --mode dev",
        "build": "vue-cli-service build --mode production",
        "lint": "vue-cli-service lint --mode production",
        "analyzer": "use_analyzer=true npm run build"
    },
    "dependencies": {
        "ali-oss": "^6.1.1",
        "async-validator": "^1.11.0",
        "axios": "^0.18.0",
        "js-cookie": "^2.2.0",
        "js-md5": "^0.7.3",
        "vant": "^2.5.0",
        "vue": "^2.6.11",
        "vue-qr": "^2.2.1",
        "vue-router": "^3.1.3",
        "vuex": "^3.1.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.12.1",
        "@vue/cli-plugin-eslint": "^3.5.1",
        "@vue/cli-service": "^5.0.8",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.0.1",
        "babel-plugin-import": "^1.11.0",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0",
        "less": "^3.9.0",
        "less-loader": "^4.1.0",
        "vue-template-compiler": "^2.5.21",
        "webpack-bundle-analyzer": "^3.3.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "engines": {
        "node": ">= 10.15.0",
        "npm": ">= 6.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
